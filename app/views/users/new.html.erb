<% if @users.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@users.errors.count, "error") %>
      prohibited you from saving:
    </h2>

    <ul>
    <% @users.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>

<%= form_with model: @users, local: true do |f|%>
    <%=f.label :email%>
    <%=f.text_field :email%>
    <%=f.label :password%>
    <%=f.password_field :password%>
    <%=f.submit%>

<%end%>